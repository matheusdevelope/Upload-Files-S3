{
  "name": "Upload-Files-S3",
  "version": "1.0.0",
  "main": "./src/index.ts",
  "repository": "https://github.com/matheusdevelope/Upload-Files-S3.git",
  "author": "Matheus <emersonmatheustu@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "ts-node-dev --transpile-only src/index.ts",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "typeorm:create": "yarn typeorm migration:create src/database/migration/MigrationName",
    "typeorm:generate": "yarn typeorm migration:generate -d src/database/data-source.ts src/database/migration/Migration",
    "typeorm:run": "yarn typeorm migration:run -d src/database/data-source.ts",
    "typeorm:sync": "yarn typeorm schema:sync -d src/database/data-source.ts",
    "typeorm:drop": "yarn typeorm schema:drop -d src/database/data-source.ts",
    "typeorm:starttables": "yarn typeorm:generate & yarn typeorm:run"
  },
  "dependencies": {
    "aws-sdk": "^2.1088.0",
    "basic-ftp": "^4.6.6",
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "mime-types": "^2.1.35",
    "pino": "^8.0.0",
    "pino-pretty": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.8",
    "typeorm": "^0.3.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.23",
    "@types/uuid": "^8.3.4",
    "typescript": "^4.7.4"
  }
}
